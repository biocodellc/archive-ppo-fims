@prefix dwc:     <http://rs.tdwg.org/dwc/terms/> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ark:     <http://biscicol.org/id/ark:> .
@prefix obo:     <http://purl.obolibrary.org/obo/> .

# logical axiom to infer vascular leaf phenological stage:
#'plant phenological stage'
# and ('has participant' some 
#    ('whole plant'
#     and ('has quality' some 'leaves present')))

# We want to infer instances of "'whole plant' that 'participates in' some 'vascular leaf phenological stage'"

# leaves present:
#'leaf presence'
# and ('quality of' some 
#    ('whole plant'
#     and ('has part' some 'true leaf')))

#<http://purl.obolibrary.org/obo/RO_0000056> #'participates in'

# Need to declare these as objectProperties here or else protege does not figure it out
obo:RO_0000086 a owl:ObjectProperty .
obo:RO_0000057 a owl:ObjectProperty .
obo:BFO_0000051 a owl:ObjectProperty .

# Name the ontology
<http://biscicol.org/test/?testOntology=123456> rdf:type owl:Ontology  .

<http://biscicol.org/test/?plant1>
      	a obo:PO_0000003;  				 # 'whole plant'
        obo:RO_0000086  <urn:traitInstanceClass=1>; # 'has quality' some 'leaves present'
	rdfs:label "plant1" .

<http://biscicol.org/test/?plant2>
      	a obo:PO_0000003;  				 # 'whole plant'
        obo:RO_0000086  <urn:traitInstanceClass=2>; # 'has quality' some 'senescent leaves present'
        obo:RO_0000086  <urn:traitInstanceClass=3>; # 'has quality' some 'expanding leaves present'
	rdfs:label "plant2" .

<urn:traitInstanceClass=1> 
	a obo:PPO_0003002 ; 			# 'leaves present'
	rdfs:label "instance of leaves present (trait1)" .

<urn:traitInstanceClass=2> 
	a obo:PPO_0003005 ; 			# 'senescent leaves present'
	rdfs:label "instance of 'senescent leaves present' (trait2)" .

<urn:traitInstanceClass=3> 
	a obo:PPO_0003003 ; 			# 'expanding leaves present'
	rdfs:label "instance of 'expanding leaves present' (trait3)" .
